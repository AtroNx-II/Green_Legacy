script "PlatFormMover" (int Distance,int Speed,int StayDelay) 
{

    int RESTARTANIMATION,MoveDistance,FirstX,SecondX,FirstY,SecondY,FirstZ,SecondZ,Xdiff,Ydiff,Zdiff,platform_Height,platform_Radius,player_radius,platform_Radius2,player_height,FDiffDist;
    IF(RESTARTANIMATION==0) //PREVENT CHANGE TID CONTINUOSLY 
    {
        int MissileID = UniqueTid();//Acs_NamedExecuteWithResult("TidCaller",0,2000,0,0);
        Thing_ChangeTID(0, MissileID);
        SetActivator(1001+PlayerNumber(), AAPTR_PLAYER1);
        platform_Height = GetActorProperty(MissileID, APROP_Height);
        platform_Radius = GetActorProperty(MissileID, APROP_Radius)+player_radius;
        player_radius = GetActorProperty(APROP_TargetTID, APROP_Radius)/2;
		player_height = GetActorProperty(APROP_TargetTID, APROP_Height);
    }
    
        For(MoveDistance = Distance*1.0; MoveDistance >= 0.0; MoveDistance -= 1.0) // MOVE DISTANCE
        { 
    FirstX=GetActorX(MissileID);
    FirstY=GetActorY(ActivatorTid());
    FirstZ=GetActorZ(ActivatorTid())-GetActorFLOORZ(ActivatorTid());
    SetActorVelocity(MissileID,Speed*1.0,0,0,False,False);
		ModifyPlayerFriction(MissileID, 0.55);
            Delay(1);
    SecondX=GetActorX(ActivatorTid());
    SecondY=GetActorY(ActivatorTid());
    SecondZ=GetActorZ(ActivatorTid())-GetActorFloorZ(ActivatorTid());
    Xdiff = FirstX-SecondX;
    Ydiff = FirstY-SecondY;
    Zdiff = FirstZ-SecondZ;
if(GetActorX(MissileID)-GetActorX(ActivatorTid())< platform_Radius && GetActorZ(MissileID)+platform_Height==GetActorZ(ActivatorTid()))
        {
        SetActorPosition(ActivatorTid(),GetActorX(MissileID)-Xdiff,GetActorY(MissileID),GetActorZ(ActivatorTid()),False);
				ModifyPlayerFriction(ActivatorTid(), 0.55);
        }
        }
    Thing_Stop(MissileID);
                    Delay(StayDelay);
        For(MoveDistance = Distance*1.0; MoveDistance >= 0.0; MoveDistance -= 1.0)
        { 
    FirstX=GetActorX(MissileID);
    FirstY=GetActorY(ActivatorTid());
    FirstZ=GetActorZ(ActivatorTid())-GetActorFLOORZ(ActivatorTid());
        SetActorVelocity(MissileID,-Speed*1.0,0,0,False,False);
		ModifyPlayerFriction(MissileID, 0.55);
        Delay(1);
    SecondX=GetActorX(ActivatorTid());
    SecondY=GetActorY(ActivatorTid());
    SecondZ=GetActorZ(ActivatorTid())-GetActorFloorZ(ActivatorTid());
    Xdiff = FirstX-SecondX;
    Ydiff = FirstY-SecondY;
    Zdiff = FirstZ-SecondZ;
	if(GetActorX(MissileID)-GetActorX(ActivatorTid())< platform_Radius && GetActorZ(MissileID)+platform_Height==GetActorZ(ActivatorTid()))
        {
        SetActorPosition(ActivatorTid(),GetActorX(MissileID)-Xdiff,GetActorY(MissileID),GetActorZ(ActivatorTid()),False);
		ModifyPlayerFriction(ActivatorTid(), 0.55);
        }

		
        }
        Thing_Stop(MissileID);
                    Delay(StayDelay);
RESTARTANIMATION=1;
          restart;
}

