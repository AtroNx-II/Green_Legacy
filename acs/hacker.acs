// check if player is on ground
function int CheckPlayerOnGround(void)
{
  int onground;
  if(GetActorZ (0) == GetActorFloorZ (0) ||
       (GetActorVelZ(0) == 0 && !Spawn("Z_FloorCheck", GetActorX(0), GetActorY(0), GetActorZ(0) - (4 << 16), 0, GetActorAngle(0)))
       )
  {
    onground = 1;
    //SetFont("smallfont");
    //Print(s: "on ground");
  }
  else
  {
    onground = 0;
    //SetFont("smallfont");
    //Print(s: "in air");
  }
  return onground;
}

