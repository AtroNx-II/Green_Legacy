    A_FireProjectile("GLShot",-90,0,40);A_FireProjectile("GreenPlasmaMuzzle_R",-90,0,50); }
    else { A_FireProjectile("GLShot",90,0,-40);A_FireProjectile("GreenPlasmaMuzzle_L",90,0,-50); }
    }
    }
    }
}
