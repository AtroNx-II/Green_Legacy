ACTOR WaitingM16
{
  Radius 20
  Height  40
  +NOBLOCKMAP
   +noclip
   +FLOORCLIP
    scale 1.0
  States
  {
  Spawn:
        TNT1 A 0 A_JumpIfInventory("BackwardsCheck",1,"SpawnLeft")
    SpawnRight:
	PSTD A 0 A_SetScale(1.0,1.0)
        PSTD A 1 
    {
	if(CountInv("PlayingEmily",AAPTR_MASTER)==0)
	{
	Return State("Death");
	}
	A_FaceMaster;
    ACS_NamedExecutealways("BackwardsCheck",0,0,0,0);
    if (CountInv("BackwardsCheck") > 0) { return state("SpawnLeft");}
    return state("");
    }
        loop
    SpawnLeft:
	PSTD A 0 A_SetScale(-1.0,1.0)
        PSTD A 1 
    {   
	if(CountInv("PlayingEmily",AAPTR_MASTER)==0)
	{
	Return State("Death");
	}
	A_FaceMaster;
    ACS_NamedExecutealways("BackwardsCheck",0,0,0,0);
    if (CountInv("BackwardsCheck") ==0) { return state("SpawnRight");}
    return state("");
    }
        loop
  Death:
    TNT1 A 1
    Stop
  }
}
